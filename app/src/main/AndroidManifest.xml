<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="androidapp.yashthaluri.com.khatabook">

    <application
        android:allowBackup="true"
        android:icon="@drawable/iconkhatabook"
        android:label="@string/app_name"
        android:roundIcon="@drawable/iconkhatabook"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".ProfileActivty"></activity>
        <activity android:name=".AddCustomerActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".AuthencationActivity" />





        <activity android:name=".MainActivity" />
